<h1 class="text-center">My Uploaded Files</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <% if (files) { %>
    <div class="list-group">
      <% for (file of files) { %>
      <a href="/csv/<%= file.id %>/view" class="list-group-item">
        <%= file.name %>
      </a>
      <% } %>
    </div>
    <% } else { %>
    <p>No files uploaded yet.</p>
    <% } %>
  </div>
</div>
